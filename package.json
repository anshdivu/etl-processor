{
  "name": "etl-processor",
  "version": "1.0.0",
  "main": "index.ts",
  "author": "Diviyansh Bhatnagar",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "test": "jest --watch"
  },
  "dependencies": {
    "rxjs": "^6.2.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "jest": "^23.4.2",
    "prettier": "^1.13.7",
    "ts-jest": "^23.0.1",
    "typescript": "^2.9.2"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts|js)?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
